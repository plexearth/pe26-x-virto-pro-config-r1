  "sysConditionSets": {
    "IS_GOOD_MAP_EXPLORER": {
      "conditions": [
        {
          "type": "version",
          "operator": ">",
          "value": "[$mapxVersion]",
          "compareTo": "2.1"
        }
      ]
    },
    "IS_SESSION_REM2": {
      "conditions": [
        {
          "type": "int",
          "operator": "==",
          "value": "[func:[getSessionsRem2]]",
          "compareTo": "0"
        }
      ]
    },
    "IS_SESSION_REM3": {
      "conditions": [
        {
          "type": "int",
          "operator": "==",
          "value": "[func:[getSessionsRem3]]",
          "compareTo": "0"
        }
      ]
    }
  },
