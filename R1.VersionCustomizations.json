      "type": "datetime.now",
      "isUtc": "true",
      "fmt": "yyyy-MM-ddTHH:mm:ss.fff"
    },
    "getTodayUtc": {
      "type": "datetime.now",
      "isUtc": "true",
      "fmt": "yyyy-MM-dd"
    },
    "getDaysToExpire": {
      "type": "datetime.span",
      "end": "[$expirationDate]",
      "start": "[func:[getTodayUtc]]",
      "fmt": "%d"
    },
    "getSessionsRem2": {
      "type": "num.op",
      "oper": "%",
      "num1": "[$sessions]",
      "num2": "2"
    },
    "getSessionsRem3": {
      "type": "num.op",
      "oper": "%",
